[Unit]
Description=gunicorn 2-hbnb systemd service.

[Service]
Type=simple
ExecStart=/bin/bash/env bash -c '/home/ubuntu/.local/bin/gunicorn --bind 0.0.0.0:5003 web_dynamic.2-hbnb:app \
    -w 4 --access-logfile /tmp/airbnb-access.log --error-logfile /tmp/airbnb-error.log'

[Install]
WantedBy=multi-user.target
